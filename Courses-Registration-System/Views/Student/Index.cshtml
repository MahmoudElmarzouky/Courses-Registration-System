@model IQueryable<StudentViewModel>
<table class="table">
    <thead>
        <tr>
            <th>
                Student Name
            </th>
            <th>
                Student Email
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StudentName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StudentEmail)
            </td>
        </tr>
}
    </tbody>
</table>
